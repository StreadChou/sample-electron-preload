{
  "name": "@stread/sample-electron-preload",
  "version": "0.0.1",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "patch": "npm run build && npm version patch --no-git-tag-version",
    "minor": "npm run build && npm version minor --no-git-tag-version",
    "major": "npm run build && npm version major --no-git-tag-version"
  },
  "exports": {
    "./main": {
      "types": "./dist/main/index.d.ts",
      "require": "./dist/main/index.js",
      "default": "./dist/main/index.js"
    },
    "./renderer": {
      "types": "./dist/renderer/index.d.ts",
      "require": "./dist/renderer/index.js",
      "default": "./dist/renderer/index.js"
    },
    "./preload": {
      "types": "./dist/preload/index.d.ts",
      "require": "./dist/preload/index.js",
      "default": "./dist/preload/index.js"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.1.6",
    "electron": "^37.2.6"
  },
  "peerDependencies": {
    "electron": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
