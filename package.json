{
  "name": "@stread/sample-electron-preload",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && tsup",
    "patch": "npm run build && npm version patch --no-git-tag-version",
    "minor": "npm run build && npm version minor --no-git-tag-version",
    "major": "npm run build && npm version major --no-git-tag-version"
  },
  "exports": {
    "./main": {
      "types": "./dist/main/index.d.ts",
      "import": "./dist/main/index.js",
      "require": "./dist/main/index.cjs"
    },
    "./renderer": {
      "types": "./dist/renderer/index.d.ts",
      "import": "./dist/renderer/index.js",
      "require": "./dist/renderer/index.cjs"
    },
    "./preload": {
      "types": "./dist/preload/index.d.ts",
      "import": "./dist/preload/index.js",
      "require": "./dist/preload/index.cjs"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "electron": "^37.2.6",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "electron": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "tsup": "^8.5.0"
  }
}
